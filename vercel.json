{
  "buildCommand": "pnpm nx build frontend",
  "outputDirectory": "apps/frontend/.next",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD apps/frontend/",
  "functions": {
    "apps/frontend/.next/server/pages/*.js": {
      "includeFiles": "apps/frontend/public/**"
    }
  }
}